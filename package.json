{
  "name": "@mohatt/website",
  "private": true,
  "description": "Codebase for my personal website",
  "version": "2.0.0",
  "sideEffects": [
    "src/**/*.css"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "pnpm run lint:js && pnpm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.css'",
    "eslint:config": "eslint --print-config .eslintrc.json",
    "eslint:check": "eslint-config-prettier .eslintrc.json",
    "fonts:preload-cache": "gatsby-preload-fonts"
  },
  "keywords": ["website", "gatsby", "react"],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/website.git"
  },
  "homepage": "https://github.com/mohatt/website",
  "bugs": "https://github.com/mohatt/website/issues",
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@tailwindcss/typography": "0.4.0",
    "autoprefixer": "10.2.5",
    "classnames": "2.3.1",
    "gatsby": "3.3.1",
    "gatsby-plugin-advanced-pages": "2.2.1",
    "gatsby-plugin-catch-links": "3.3.0",
    "gatsby-plugin-google-gtag": "3.3.0",
    "gatsby-plugin-manifest": "3.3.0",
    "gatsby-plugin-mdx": "2.3.0",
    "gatsby-plugin-postbuild": "2.2.2",
    "gatsby-plugin-postcss": "4.3.0",
    "gatsby-plugin-preload-fonts": "2.3.0",
    "gatsby-plugin-react-helmet": "4.3.0",
    "gatsby-plugin-robots-txt": "1.5.6",
    "gatsby-plugin-sharp": "3.3.1",
    "gatsby-plugin-sitemap": "3.3.0",
    "gatsby-remark-images": "5.0.0",
    "gatsby-remark-prismjs": "5.0.0",
    "gatsby-remark-smartypants": "4.0.0",
    "gatsby-source-filesystem": "3.3.0",
    "gatsby-transformer-sharp": "3.3.0",
    "gatsby-transformer-yaml": "3.3.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "postcss": "8.2.10",
    "postcss-import": "14.0.1",
    "postcss-url": "10.1.3",
    "postcss-preset-env": "6.7.0",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "tailwindcss": "2.1.1"
  },
  "devDependencies": {
    "@mohatt/eslint-config-gatsby-standard": "2.0.1",
    "@types/tailwindcss": "2.0.2",
    "eslint": "7.20.0",
    "prettier": "2.2.1",
    "simple-icons": "4.21.0",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "21.0.0",
    "webpack-bundle-analyzer": "4.4.1"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "config:js-app"
    ]
  }
}
