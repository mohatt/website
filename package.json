{
  "name": "@mohatt/website",
  "private": true,
  "description": "Codebase for my personal website",
  "version": "2.0.0",
  "sideEffects": [
    "src/**/*.css"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:profile": "gatsby build --profile",
    "serve": "gatsby serve",
    "lint": "pnpm run lint:js && pnpm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.css'",
    "eslint:config": "eslint --print-config .eslintrc.json",
    "eslint:check": "eslint-config-prettier .eslintrc.json",
    "fonts:preload-cache": "gatsby-preload-fonts"
  },
  "keywords": ["website", "gatsby", "react"],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/website.git"
  },
  "homepage": "https://github.com/mohatt/website",
  "bugs": "https://github.com/mohatt/website/issues",
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@tailwindcss/typography": "0.4.1",
    "autoprefixer": "10.2.6",
    "classnames": "2.3.1",
    "gatsby": "3.7.1",
    "gatsby-plugin-advanced-pages": "2.2.1",
    "gatsby-plugin-catch-links": "3.7.1",
    "gatsby-plugin-graphql-config": "0.16.1",
    "gatsby-plugin-manifest": "3.7.1",
    "gatsby-plugin-mdx": "2.7.1",
    "gatsby-plugin-postbuild": "3.0.2",
    "gatsby-plugin-postcss": "4.7.1",
    "gatsby-plugin-preload-fonts": "2.7.1",
    "gatsby-plugin-react-helmet": "4.7.1",
    "gatsby-plugin-sharp": "3.7.1",
    "gatsby-plugin-sitemap": "4.3.1",
    "gatsby-remark-images": "5.4.1",
    "gatsby-remark-prismjs": "5.4.1",
    "gatsby-remark-smartypants": "4.4.1",
    "gatsby-source-filesystem": "3.7.1",
    "gatsby-transformer-sharp": "3.7.1",
    "gatsby-transformer-yaml": "3.7.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "postcss": "8.3.3",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "10.1.3",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "tailwindcss": "2.1.4"
  },
  "devDependencies": {
    "@mohatt/eslint-config-gatsby-standard": "3.2.0",
    "@types/tailwindcss": "2.0.5",
    "eslint": "7.28.0",
    "prettier": "2.3.1",
    "simple-icons": "5.2.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "engines": {
    "node": ">=12.17.0",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "gatsby>postcss": "8.3.3"
    },
    "neverBuiltDependencies": ["fsevents"]
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not IE 11",
    "not op_mini all",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "config:js-app"
    ]
  }
}
