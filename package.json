    "eslint": "7.19.0",
    "gatsby-plugin-pnpm": "1.2.5",
    "prettier": "2.2.1",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "simple-icons": "4.9.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "config:js-app"
