{
  "name": "muhatt-site",
  "description": "Personal website based on Gatsby",
  "version": "1.0.0",
  "main": "n/a",
  "scripts": {
    "clean": "rimraf .cache public",
    "start": "yarn clean && gatsby develop -H 0.0.0.0",
    "build": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "lint": "concurrently 'yarn run lint:js' 'yarn run lint:css'",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.css'",
    "test": "jest --runInBand --config ./test/jest-config.js",
    "test:coverage": "jest --runInBand --coverage --config ./test/jest-config.js",
    "test:watch": "jest --runInBand --watch --config ./test/jest-config.js",
    "eslint:config": "eslint --print-config .eslintrc.json",
    "eslint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check"
  },
  "repository": "https://github.com/muhatt/website",
  "author": "Mohamed Elkholy <muhatt@pm.me>",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@mdx-js/mdx": "1.3.1",
    "@mdx-js/react": "1.3.1",
    "bluebird": "3.5.5",
    "classnames": "2.2.6",
    "gatsby": "2.13.80",
    "gatsby-plugin-advanced-pages": "1.3.5",
    "gatsby-plugin-catch-links": "2.1.5",
    "gatsby-plugin-google-gtag": "1.1.6",
    "gatsby-plugin-manifest": "2.2.9",
    "gatsby-plugin-mdx": "1.0.30",
    "gatsby-plugin-postcss": "2.1.5",
    "gatsby-plugin-purgecss": "4.0.0",
    "gatsby-plugin-react-helmet": "3.1.5",
    "gatsby-plugin-sharp": "2.2.17",
    "gatsby-plugin-sitemap": "2.2.9",
    "gatsby-remark-images": "3.1.17",
    "gatsby-remark-prismjs": "3.3.9",
    "gatsby-remark-smartypants": "2.1.5",
    "gatsby-source-filesystem": "2.1.16",
    "gatsby-transformer-sharp": "2.2.11",
    "gatsby-transformer-yaml": "^2.2.7",
    "invariant": "2.2.4",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "rfs": "8.0.4",
    "tailwindcss": "1.1.2",
    "tailwindcss-transitions": "2.0.1",
    "typed.js": "2.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@mohatt/eslint-config-gatsby-standard": "1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-preset-gatsby": "0.2.11",
    "concurrently": "4.1.2",
    "eslint": "6.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
