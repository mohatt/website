    "gatsby-transformer-sharp": "2.12.0",
    "gatsby-transformer-yaml": "2.11.0",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "postcss": "8.2.6",
    "postcss-import": "14.0.0",
    "postcss-preset-env": "6.7.0",
    "autoprefixer": "10.2.4",
    "prismjs": "1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "rfs": "9.0.3",
