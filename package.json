{
  "name": "@mohatt/website",
  "private": true,
  "description": "Codebase for my personal website",
  "version": "2.0.0",
  "sideEffects": [
    "src/**/*.css"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "build:profile": "gatsby build --profile",
    "serve": "gatsby serve -H 0.0.0.0",
    "lint": "pnpm run lint:js && pnpm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.css'",
    "eslint:config": "eslint --print-config .eslintrc.json",
    "eslint:check": "eslint-config-prettier .eslintrc.json",
    "fonts:preload-cache": "gatsby-preload-fonts"
  },
  "keywords": ["website", "gatsby", "react", "tailwindcss"],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/website.git"
  },
  "homepage": "https://github.com/mohatt/website",
  "bugs": "https://github.com/mohatt/website/issues",
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "autoprefixer": "10.3.3",
    "clsx": "1.1.1",
    "gatsby": "3.12.1",
    "gatsby-plugin-advanced-pages": "3.0.1",
    "gatsby-plugin-graphql-config": "0.21.0",
    "gatsby-plugin-image": "1.12.0",
    "gatsby-plugin-manifest": "3.12.0",
    "gatsby-plugin-mdx": "2.12.0",
    "gatsby-plugin-postbuild": "3.0.2",
    "gatsby-plugin-postcss": "4.12.0",
    "gatsby-plugin-preload-fonts": "2.12.0",
    "gatsby-plugin-react-helmet-async": "1.2.0",
    "gatsby-plugin-sharp": "3.12.0",
    "gatsby-plugin-sitemap": "4.8.0",
    "gatsby-remark-smartypants": "4.9.0",
    "gatsby-source-filesystem": "3.12.0",
    "gatsby-transformer-sharp": "3.12.0",
    "gatsby-transformer-yaml": "3.12.0",
    "lodash": "4.17.21",
    "photoswipe": "github:dimsemenov/photoswipe#v5-beta",
    "postcss": "8.3.6",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.1.2",
    "tailwindcss": "2.2.8"
  },
  "devDependencies": {
    "@mohatt/eslint-config-gatsby-standard": "3.2.0",
    "@types/tailwindcss": "2.2.1",
    "eslint": "7.32.0",
    "prettier": "2.3.2",
    "simple-icons": "5.11.0",
    "stylelint": "15.10.1",
    "stylelint-config-standard": "22.0.0",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "engines": {
    "node": ">=12.17.0",
    "pnpm": ">=6"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not IE 11",
    "not op_mini all",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "config:js-app"
    ]
  }
}
